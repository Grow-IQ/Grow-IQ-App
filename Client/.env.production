# Production Environment Variables
VITE_APP_NAME=GrowIQ
VITE_APP_VERSION=0.0.0
VITE_NODE_ENV=production

# Firebase Configuration - Production
# Note: Replace these with your actual production Firebase config
VITE_FIREBASE_API_KEY=your_production_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your-production-domain.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-production-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-production-bucket.firebasestorage.app
VITE_FIREBASE_MESSAGING_SENDER_ID=your_production_sender_id
VITE_FIREBASE_APP_ID=your_production_app_id
VITE_FIREBASE_MEASUREMENT_ID=your_production_measurement_id

# API Configuration
VITE_API_BASE_URL=https://your-production-api.com/api
VITE_API_TIMEOUT=15000

# Debug Settings
VITE_DEBUG_MODE=false
VITE_ENABLE_LOGGING=false
